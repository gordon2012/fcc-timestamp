{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@now/static-build" },
    { "src": "public/server.js", "use": "@now/node-server" }
  ],

  "routes": [
    {
      "src": "/timestamp/:input",
      "dest": "/public/server.js",
      "methods": ["GET"]
    },
    {
      "src": "/timestamp",
      "dest": "/public/server.js",
      "methods": ["GET"]
    }
  ]
}
