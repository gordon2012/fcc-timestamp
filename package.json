{
    "name": "fcc-timestamp",
    "version": "1.0.0",
    "description": "<https://timestamp.gordondoskas.com>",
    "main": "index.js",
    "scripts": {
        "clean": "rm -rf ./build",
        "build:server": "webpack --env.NODE_ENV=production --config webpack.config.server.js",
        "build:client": "webpack --env.NODE_ENV=production --config webpack.config.client.js",
        "build": "npm run clean && npm run build:server && npm run build:client",
        "start:server": "webpack --config webpack.config.server.js",
        "start:client": "webpack-dev-server --config webpack.config.client.js",
        "start": "npm run clean && npm-run-all --parallel start:server start:client",
        "stage": "npm run build && now",
        "deploy": "npm run build && now --prod"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gordon2012/fcc-timestamp.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/gordon2012/fcc-timestamp/issues"
    },
    "homepage": "https://github.com/gordon2012/fcc-timestamp#readme",
    "dependencies": {
        "babel-preset-react": "^6.24.1",
        "cors": "^2.8.5",
        "express": "^4.16.4",
        "react": "^16.6.3",
        "react-dom": "^16.6.3"
    },
    "devDependencies": {
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.5",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-preset-env": "^1.7.0",
        "css-loader": "^1.0.1",
        "npm-run-all": "^4.1.5",
        "react-hot-loader": "^4.5.1",
        "start-server-webpack-plugin": "^2.2.5",
        "style-loader": "^0.23.1",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.8.2",
        "webpack-node-externals": "^1.7.2"
    }
}
